# PrOgRaMs
/*
Reversee a given string
*/
import java.util.*;
import java.util.Scanner;
public class Sri {
    
        
    
  public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       int no=sc.nextInt();
       int sum=0;
       int temp=no;
       while(no>0){
           int rem=no%10;
           sum=(sum*10)+rem;
           no=no/10;
           
           
       }
       System.out.println(sum);
      
  }
}
      
