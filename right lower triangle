# PrOgRaMs
/*
  right lower triangle

*/
public class Sri
{ 
    
    public static void main(String args[]) 
    { 
        int n = 5,i,j,k; 
        for(i=1;i<5;i++)
        {
            for(j=1;j<=i;j++)
            {
                System.out.print(" ");
            }
            for(k=4;k>=i;k--){
                System.out.print("*");
            }
             System.out.println("\n");
        }
        
        
        } 
    } 
