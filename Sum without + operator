# PrOgRaMs
/*
Sum without + operator
*/
import java.util.*;
import java.util.Scanner;
public class Sri {
 static int sum(int x,int y){
     int hid;
     while(y!=0){
         hid=x &y;
         x=x^y;
         y=hid<<1;
         
     }
     return x;
    
}
    
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=sc.nextInt();
       int sum=sum( a, b);
       System.out.println("sum is"+sum);
       
    }
    
}
